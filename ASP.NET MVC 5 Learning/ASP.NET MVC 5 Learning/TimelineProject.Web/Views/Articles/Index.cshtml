@model IEnumerable<TimelineProject.Data.Models.Article>
@{
    ViewBag.Title = "Index";
}

<h2>Articles</h2>

<table class="table table-hover table-striped">
    @foreach (TimelineProject.Data.Models.Article article in Model)
    {
    <tr>
        <td>@article.Title</td>
        <td>@article.Description</td>
    </tr>
    }
</table>